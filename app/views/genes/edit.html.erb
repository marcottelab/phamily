<h1>Editing gene</h1>

<% form_for(@gene) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :original_id %><br />
    <%= f.text_field :original_id %>
  </p>
  <p>
    <%= f.label :species %><br />
    <%= f.text_field :species %>
  </p>
  <p>
    <%= f.label :symbol %><br />
    <%= f.text_field :symbol %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @gene %> |
<%= link_to 'Back', genes_path %>