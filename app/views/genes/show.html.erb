<p>
  <b>Unique Identifier:</b>
  <%=h @gene.original_id %>
</p>

<p>
  <b>Species:</b>
  <%=h @gene.species %>
</p>

<%- unless @gene.symbol.nil? -%>
<p>
  <b>Symbol:</b>
  <%=h @gene.symbol %>
</p>
<%- end -%>

<table>
<%= render :partial => 'assignment_th' %>
<%- @assignments.each do |assignment| -%>
  <%= render :partial => 'assignment', :locals => {:assignment => assignment} %>
<%- end -%>
</table>

<p>
  <b>Phenotypes</b>
  <ul><%- @phenotypes.each do |phenotype| -%>
    <li><%= link_to h(phenotype.desc), phenotype %></li>
  <%- end -%></ul>
</p>


<%= link_to 'Edit', edit_gene_path(@gene) %> |
<%= link_to 'Back', genes_path %>